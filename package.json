    "@react-navigation/stack": "^6.3.4",
    "@reduxjs/toolkit": "^1.9.0",
    "@types/react-native-dotenv": "^0.2.0",
    "cross-env": "^7.0.3",
    "expo": "~47.0.6",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "~0.15.5",
    "jwt-decode": "^3.1.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-dotenv": "^3.4.2",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-reanimated": "~2.12.0",
